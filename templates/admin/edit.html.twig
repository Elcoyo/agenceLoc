{% extends "base.html.twig" %}

{% block body %}
<h1>Modification du formulaire</h1>
    <div class="container ">
    {{form_start(form)}}
    {{form_row(form.titre)}}
    {{form_row(form.marque)}}
    {{form_row(form.modele)}}
    {{form_row(form.photo)}}
    {{form_row(form.prixJournalier)}}
    {{form_row(form.description)}}
    <button type="submit" class="btn btn-primary">Modifier</button>
    {{form_end(form)}}

    </div>

{% endblock %}